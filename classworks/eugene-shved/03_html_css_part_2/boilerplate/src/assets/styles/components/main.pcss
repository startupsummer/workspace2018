main {
  width: 100%;

  .tooltip {
    margin-left: auto;
    padding-left: 20px;
    padding-top: 20px;
  }

  .tooltip_link {
    color: #fff;
  }

  .inputs {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    margin-top: 40px;
    margin-bottom: 40px;
  }

  .inputs-item {
    width: 48%;
    border-radius: 7px;
  }

  .submit {
    border: solid #7dafab;
    border-radius: 10px;
    padding-right: 20px;
    padding-left: 20px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: #7dafab;
    margin-bottom: 20px;
    margin-top: 20px;
    color: #fff;
  }

  .header {
    background-image: url("/images/banner.jpg");
  }

  .portfolio {
    background-color: #f1f6f6;
    padding-top: 100px;
  }
}

@media (min-width: 900px) {
  main {
    margin-left: 400px;
    .contact {
      padding: 100px;
    }
    .tooltip {
      display: none;
    }
  }
}